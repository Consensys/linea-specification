% DEFAULT VALUES
\presetkeys [PRC] {var} {
	anchorRow = \missingParameter,
	relOffset = \missingParameter,
	% argOneHi  = \missingParameter,
	% argOneLo  = \missingParameter,
	% argTwoHi  = \missingParameter,
	% argTwoLo  = \missingParameter,
	argOneThree = \missingParameter,
	argOneTwo   = \missingParameter,
	argOneOne   = \missingParameter,
	argOneZero  = \missingParameter,
	argTwoThree = \missingParameter,
	argTwoTwo   = \missingParameter,
	argTwoOne   = \missingParameter,
	argTwoZero  = \missingParameter,
}{}



\newcommand{\callToWellFormedCoordinatesBlsName} {\texttt{callToWellFormedCoordinates}}
\newcommand{\callToWellFormedCoordinatesBls} [1] {
	\setkeys[PRC]{var}{#1}
	\callToWellFormedCoordinatesBlsName _{\cmdPRC@var@anchorRow}
	\left[ \begin{array}{ll}
			\utt{Rel. row offset:}  & \cmdPRC@var@relOffset    \\
			\utt{First argument 3:} & \cmdPRC@var@argOneThree  \\
			\utt{First argument 2:} & \cmdPRC@var@argOneTwo    \\
			\utt{First argument 1:} & \cmdPRC@var@argOneOne    \\
			\utt{First argument 0:} & \cmdPRC@var@argOneZero   \\
			\utt{Second argument 3:} & \cmdPRC@var@argTwoThree  \\
			\utt{Second argument 2:} & \cmdPRC@var@argTwoTwo    \\
			\utt{Second argument 1:} & \cmdPRC@var@argTwoOne    \\
			\utt{Second argument 0:} & \cmdPRC@var@argTwoZero   \\
		\end{array} \right]
}