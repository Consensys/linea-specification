\newcommand{\standardDomSubStampsName} {\stampSignifier\texttt{standardDomSubStamps}}
\newcommand{\standardDomSubStamps} [1] {
  \setkeys[DOMSUB]{var}{#1}
  \standardDomSubStampsName _{\cmdDOMSUB@var@anchorRow}
  \left[ \begin{array}{ll}
    \utt{Rel. row offset:} & \cmdDOMSUB@var@relOffset \\
    \utt{Dom offset:}      & \cmdDOMSUB@var@domOffset \\
  \end{array} \right]
}

