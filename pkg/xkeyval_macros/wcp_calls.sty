% DEFINING KEYS

\define@cmdkey [WCP] {var} {anchorRow} {} % "i" in the spec, ALWAYS use this name
\define@cmdkey [WCP] {var} {relOffset} {} % relative offset, ALWAYS use this name
\define@cmdkey [WCP] {var} {argOneHi}  {}
\define@cmdkey [WCP] {var} {argOneLo}  {}
\define@cmdkey [WCP] {var} {argTwoHi}  {}
\define@cmdkey [WCP] {var} {argTwoLo}  {}

\define@cmdkey [WCP] {var} {argOneThree}  {}
\define@cmdkey [WCP] {var} {argOneTwo}    {}
\define@cmdkey [WCP] {var} {argOneOne}    {}
\define@cmdkey [WCP] {var} {argOneZero}   {}
\define@cmdkey [WCP] {var} {argTwoThree}  {}
\define@cmdkey [WCP] {var} {argTwoTwo}    {}
\define@cmdkey [WCP] {var} {argTwoOne}    {}
\define@cmdkey [WCP] {var} {argTwoZero}   {}

% the above defines macros Ã  la "\cmdWCP@var@anchorRow"

% DEFAULT VALUES
\presetkeys [WCP] {var} {
	anchorRow = \missingParameter,
	relOffset = \missingParameter,
	argOneHi  = \missingParameter,
	argOneLo  = \missingParameter,
	argTwoHi  = \missingParameter,
	argTwoLo  = \missingParameter,
	argOneThree = \missingParameter,
	argOneTwo   = \missingParameter,
	argOneOne   = \missingParameter,
	argOneZero  = \missingParameter,
	argTwoThree = \missingParameter,
	argTwoTwo   = \missingParameter,
	argTwoOne   = \missingParameter,
	argTwoZero  = \missingParameter,
}{}

% "call to LEQ" macro
\newcommand{\wcpCallToLeqName} {\texttt{wcpCallToLEQ}}
\newcommand{\wcpCallToLeq} [1] {
	\setkeys[WCP]{var}{#1}
	\wcpCallToLeqName _{\cmdWCP@var@anchorRow}
	\left[ \begin{array}{ll}
			\utt{Rel. row offset:} & \cmdWCP@var@relOffset \\
			\utt{1st argument hi:} & \cmdWCP@var@argOneHi  \\
			\utt{1st argument lo:} & \cmdWCP@var@argOneLo  \\
			\utt{2nd argument hi:} & \cmdWCP@var@argTwoHi  \\
			\utt{2nd argument lo:} & \cmdWCP@var@argTwoLo  \\
		\end{array} \right]
}

